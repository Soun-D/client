{
  "manifest_version": 3,
  "name": "Soun-D, Sounds in life",
  "description": "Soun-D website sounds chrome extension",
  "version": "1.0",
  "background": {
    "service_worker": "./background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "identity",
    "identity.email"
  ],
  "author": "Kwak Do Hyun",
  "action": {
    "default_popup": "index.html",
    "default_title": "Soun-D",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  }
}
